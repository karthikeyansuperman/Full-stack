public class CountWords {
        public static void main(String[] args)
        {
         String x="india is my country";
         String y[]=x.split(" ");
         for(int i=0;i<y.length;i++)
         {
             System.out.println(y[i]);
         }
            System.out.println("String word count is "+y.length);
        }

    }
