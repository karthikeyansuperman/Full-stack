public class Main {
  public static void main(String[] args) {
  
  int n=48;
  for(int i=1;i<=n;i=i+1)
  {
  if(n%i==0)
  {
  System.out.println("The all faactors of "+i);
  }
  }
  }
}

--------------------------------------------------

public class Main {
  public static void main(String[] args) {
  int n=48;
  int sum=0;
  for(int i=1;i<=n;i=i+1)
  {
  if(n%i==0)
  {
  System.out.println("The all faactors of "+i);
  sum=sum+i;
  System.out.println(sum);
  }
  }
  }
}
