import java.util.Scanner;
public class WordOccurance {
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter a sentence");
        String x=sc.nextLine();
        String y[]=x.split(" ");
        int counter=0;
        for(String z:y)
        {
            if(z.equals("sea"))
            {
                counter++;
            }
        }
        System.out.println(counter);
    }
}
